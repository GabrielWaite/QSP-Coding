Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"Expressing", " ", "sin", 
     RowBox[{"(", "nt", ")"}], " ", "as", " ", "a", " ", "polynomial", " ", 
     "of", " ", "sin", 
     RowBox[{"(", "t", ")"}], " ", "is", " ", "not", " ", "as", " ", 
     "straightforward", " ", "as", " ", "it", " ", "was", " ", "for", " ", 
     "cos", 
     RowBox[{"(", "nt", ")"}]}], ",", " ", 
    RowBox[{
    "so", " ", "we", " ", "use", " ", "3", " ", "steps", " ", "to", " ", 
     "get", " ", "that"}]}], "*)"}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"expr1", "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["U", "\[Theta]"], "[", 
        RowBox[{"[", "1", "]"}], "]"}], "[", 
       RowBox[{"[", "2", "]"}], "]"}], " ", "//", "Simplify"}]}], ";", 
    "      ", 
    RowBox[{"(*", 
     RowBox[{"Extracting", " ", "the", " ", "first", " ", "element"}], "*)"}],
     "\[IndentingNewLine]", 
    RowBox[{"expandedExpr1", "=", 
     RowBox[{"Expand", "[", "expr1", "]"}]}], ";"}], "     ", "\n", 
   RowBox[{
    RowBox[{"exprTransformed1", "=", 
     RowBox[{"expandedExpr1", "/.", " ", 
      RowBox[{
       RowBox[{
        RowBox[{"Cos", "[", "a_", "]"}], " ", 
        RowBox[{"Sin", "[", 
         RowBox[{"t", "/", "2"}], "]"}]}], ":>", 
       RowBox[{
        RowBox[{"1", "/", "2"}], " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Sin", "[", 
           RowBox[{"a", "-", 
            RowBox[{"t", "/", "2"}]}], "]"}], "+", 
          RowBox[{"Sin", "[", 
           RowBox[{"a", "+", 
            RowBox[{"t", "/", "2"}]}], "]"}]}], ")"}]}]}]}]}], ";"}], " ", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{
     "1.", " ", "Converting", " ", "the", " ", "term", " ", "as", " ", "the", 
      " ", "polynomial", " ", "of", " ", "sin", 
      RowBox[{"(", 
       RowBox[{"t", "/", "2"}], ")"}], " ", "following", " ", "the", " ", 
      "identity", " ", "sin", 
      RowBox[{"(", "a", ")"}], "cos", 
      RowBox[{"(", "b", ")"}]}], "=", 
     RowBox[{
      RowBox[{"1", "/", "2"}], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"sin", 
         RowBox[{"(", 
          RowBox[{"a", "-", 
           RowBox[{"b", "/", "2"}]}], ")"}]}], "+", 
        RowBox[{"sin", 
         RowBox[{"(", 
          RowBox[{"a", "+", 
           RowBox[{"b", "/", "2"}]}], ")"}]}]}], ")"}]}]}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"CS", "=", 
